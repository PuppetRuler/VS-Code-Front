{"ast":null,"code":"import Cotegory from './components/Category.vue';\nexport default {\n  name: 'App',\n  components: {\n    Cotegory\n  },\n  data() {\n    return {\n      foods: ['火锅', '烧烤', '小龙虾', '牛排'],\n      games: ['红色警戒', '穿越火线', '劲舞团', '超级玛丽'],\n      films: ['《教父》', '《拆弹专家》', '《你好，李焕英》', '《尚硅谷》']\n    };\n  }\n};","map":{"version":3,"names":["Cotegory","name","components","data","foods","games","films"],"sources":["src/App.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <!-- 在App里解析之后再放到slot插槽内 -->\r\n        <Cotegory title=\"美食\">\r\n            <img slot=\"center\" src=\"https://upload.wikimedia.org/wikipedia/zh/e/ea/The_Journey_of_Elaina_01.jpg\" alt=\"临时追加的图片\">\r\n            <a slot=\"footer\" href=\"javascript:;\">更多美食</a>\r\n        </Cotegory>\r\n        <Cotegory title=\"游戏\">\r\n            <ul slot=\"center\" v-for=\"(g, index) in games\" :key=\"index\">\r\n                <li>{{ g }}</li>\r\n            </ul>\r\n            <div class=\"foot\" slot=\"footer\">\r\n                <a href=\"javascript:;\">单机游戏</a>\r\n                <a href=\"javascript:;\">网络游戏</a>\r\n            </div>\r\n        </Cotegory>\r\n        <Cotegory title=\"电影\">\r\n            <video slot=\"center\" controls src=\"http://clips.vorwaerts-gmbh.de/big_buck_bunny.mp4\"></video>\r\n            <template v-slot:footer>\r\n                <div class=\"foot\">\r\n                    <a href=\"javascript:;\">经典</a>\r\n                    <a href=\"javascript:;\">热门</a>\r\n                    <a href=\"javascript:;\">推荐</a>\r\n                </div>\r\n                <h4>欢迎前来观影</h4>\r\n            </template>\r\n        </Cotegory>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import Cotegory from './components/Category.vue';\r\n    export default {\r\n        name: 'App',\r\n        components: { Cotegory },\r\n        data() {\r\n            return {\r\n                foods: ['火锅', '烧烤', '小龙虾', '牛排'],\r\n                games: ['红色警戒', '穿越火线', '劲舞团', '超级玛丽'],\r\n                films: ['《教父》', '《拆弹专家》', '《你好，李焕英》', '《尚硅谷》']\r\n            };\r\n        }\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    .container,\r\n    .foot {\r\n        display: flex;\r\n        justify-content: space-around;\r\n    }\r\n    img {\r\n        width: 100%;\r\n    }\r\n    video {\r\n        width: 100%;\r\n    }\r\n    h4 {\r\n        text-align: center;\r\n    }\r\n</style>"],"mappings":"AA+BA,OAAAA,QAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;IACA;MACAC,KAAA;MACAC,KAAA;MACAC,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}